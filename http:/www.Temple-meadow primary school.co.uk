			#content #sidebar .widget ul li a { color:Darkblue; text-decoration:none; margin-left:-16px; padding:4px 8px 4px 16px;}
			#content #sidebar .widget ul li a:hover { color:Bl; font-weight:bold; text-decoration:none;}
			
